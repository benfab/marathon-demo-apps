{
    "id": "redis",
    "container": {
        "docker": {
            "image": "nexus.msdomain.com:14000/redis",
            "network": "BRIDGE",
            "portMappings": [{
                "containerPort": 80,
                "hostPort": 0,
                "protocol": "tcp"
            }],
            "parameters": [
             {"key": "env", "value": "SERVICE_NAME=redis"}
            ]}
    },
    "cpus": 0.1,
    "mem": 32,
    "instances": 1
}
