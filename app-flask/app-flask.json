{
    "id": "app-flask",
    "labels": {
   	"HAPROXY_0_MODE": "http",
   	"HAPROXY_GROUP": "external",
    	"SERVICE_NAME": "app-flask",
    	"MARATHON_HOSTNAME": "app-flack",
    	"HAPROXY_0_PORT": "8090",
    	"SERVICE_TAGS": "webserver",
    	"HAPROXY_0_VHOST": "app-flask.testing"
  },
    "container": {
        "docker": {
            "image": "benfab/app-flask",
            "network": "BRIDGE",
            "portMappings": [{
                "containerPort": 8359,
                "hostPort": 0,
                "protocol": "tcp"
            }]
        }
    },
    "cpus": 1,
    "mem": 128,
    "instances": 4
}
